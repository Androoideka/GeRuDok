{
	"name": "Java",
	"image": "androoideka/alpine-gui-java:latest",
	"features": {
		"ghcr.io/androoideka/containers/alpine-utils:latest": {},
		"ghcr.io/androoideka/containers/wslg-config:latest": {}
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"vscjava.vscode-java-pack"
			]
		}
	},
	"containerEnv": {
		"DISPLAY": ":0",
		"WAYLAND_DISPLAY": "${localEnv:WAYLAND_DISPLAY}",
		"XDG_RUNTIME_DIR": "${localEnv:XDG_RUNTIME_DIR}",
		"PULSE_SERVER": "${localEnv:PULSE_SERVER}"
	},
	"remoteUser": "vscode"
}